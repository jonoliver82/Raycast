    <OutputType>WinExe</OutputType>

    <!-- required for a WinForms application -->
    <UseWindowsForms>true</UseWindowsForms>

    <!-- optional, preserve the names you were using -->
    <RootNamespace>GraphicsTest</RootNamespace>
    <AssemblyName>GraphicsTest</AssemblyName>
  </PropertyGroup>

</Project>